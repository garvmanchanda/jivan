{
  "expo": {
    "name": "Jivan",
    "slug": "jivan",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jivan.app"
    },
    "android": {
      "package": "com.jivan.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-av",
        {
          "microphonePermission": "Allow Jivan to access your microphone for voice queries."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7c6371bf-cbe3-448f-a6cd-7f5cb3365f6d"
      }
    },
    "owner": "garvmanchanda"
  }
}
